    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "storybook": "start-storybook -p 6006",
    "storybook-build": "build-storybook",
    "test": "jest --ci --coverage",
    "test-e2e": "playwright test",
    "ts-check": "tsc --noEmit"
